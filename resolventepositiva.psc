Algoritmo Laresolventepositiva
	definir a, b, c como real;
	definir resultado como real;
	escribir "Introduce a: ";
	leer a;
	escribir "Introduce b: ";
	leer b;
	escribir "Introduce c: ";
	leer c;
	
	resultado<- (-b +raiz((b^2)-4*a*c))/(2*a)
	escribir "el resultado de la resolvente es: ", resultado;
	
FinAlgoritmo
